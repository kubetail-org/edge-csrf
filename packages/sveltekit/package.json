{
  "name": "@edge-csrf/sveltekit",
  "version": "2.0.0-rc9",
  "description": "Edge-CSRF SvelteKit integration library",
  "author": "Andres Morey",
  "license": "MIT",
  "repository": "kubetail-org/edge-csrf",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
