{
  "name": "@edge-csrf/node-http",
  "version": "0.0.0",
  "description": "Edge-CSRF integration library for node's http module",
  "author": "Andres Morey",
  "license": "MIT",
  "repository": "kubetail-org/edge-csrf",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
